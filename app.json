{
  "expo": {
    "name": "Saurellius Cloud Payroll",
    "slug": "saurellius-payroll",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1473FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.drpaystub.saurellius"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1473FF"
      },
      "package": "com.drpaystub.saurellius"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "saurellius-cloud-payroll"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "saurellius"
  }
}
